{
  "manifest_version": 2,

  "name": "Reg me",
  "description": "Reg me",
  "version": "2.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Reg me"
  },
  "permissions": [
    "storage",
    "tabs",
    "notifications",
    "bookmarks",
    "activeTab",
    "clipboardWrite",
    "tabCapture",
    "http://*/*",
    "https://*/*",
    "unlimitedStorage"

  ]
}